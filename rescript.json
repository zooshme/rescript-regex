{
  "name": "@zooshme/rescript-regex",
  "sources": [
    {
      "dir": "./src",
      "subdirs": true
    }
  ],
  "package-specs": [
    {
      "module": "esmodule",
      "in-source": true
    }
  ],
  "jsx": {
    "version": 4,
    "mode": "automatic"
  },
  "suffix": ".res.mjs",
  "bs-dependencies": [
    "@rescript/core"
  ],
  "bsc-flags": [
    "-open RescriptCore"
  ],
  "gentypeconfig": {
    "module": "esmodule",
    "moduleResolution": "node",
    "generatedFileExtension": ".gen.tsx"
  }
}